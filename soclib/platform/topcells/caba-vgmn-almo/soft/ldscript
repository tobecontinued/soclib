
SECTIONS
{
   . = 0x80000000;
   .excep : {
		*(.excep)
		*(.excep.*)
   }

   . = 0xbfc00000;
   .reset : {
		*(.reset)
		*(.reset.*)
   }

   . = 0x80020000;
   .text : {
      *(.text)
   }

   .rodata : {
      *(.rodata)
      *(.rodata.*)
      . = ALIGN(4);
   }
   .data  : {
      *(.data)
   }
   .sdata : {
      *(.lit8)
      *(.lit4)
      *(.sdata)
   }
   .bss   : {
      *(.bss)
      *(COMMON)
   }
   _end = .;
   _heap = .;
   _stack = 0x10100000-16;
}
