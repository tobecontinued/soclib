__id__ = "$Id: target_vci_transactor.sd 917 2009-03-12 10:10:06Z nipo $"
__version__ = "$Revision: 917 $"

Module('tlmdt:target_vci_transactor',
       classname = 'soclib::tlmdt::TargetVciTransactor',
       tmpl_parameters = [ parameter.Module('vci_param_caba',  default = 'caba:vci_param'),
                           parameter.Module('vci_param_tlmdt', default = 'tlmdt:vci_param') ],
       header_files = ['../source/include/target_vci_transactor.h',],
       implementation_files = ['../source/src/target_vci_transactor.cpp',],
       ports = [ Port('caba:vci_initiator', 'p_vci_initiator'),
                 Port('caba:bit_in', 'p_resetn', auto = 'resetn'),
                 Port('caba:clock_in', 'p_clk', auto = 'clock') ],
       uses = [  Uses('common:tlmdt'),
                 Uses('tlmdt:target_vci_transactor_buffer'),
                 Uses('caba:vci_initiator'),
                 Uses('common:mapping_table') ],
       #debug = True,
       )
